int main()
{
	//& 按位与 两个都得1
	//|按位或 只要1个为1
	//^按位异或 相同为0 不相同为1
	// += -= *= /= %= &= |= ^=  >>= <<=等等
	//!逻辑反操作 +正值 -负值 &取地址 sizeof()地址空间单位字节
	int a=1;
	a=a<<2;//向左移动两位，左边丢弃，右边补零
	printf("%d\n",a);
	//数组大小是总体大小或个体大小
	return 0;
}
